{
  "presets": ["@babel/preset-react"],
  "env": {
    "development": {
      "presets": [
        "@babel/preset-env",["@babel/preset-react", { "development": true }]
      ],
      "plugins": ["@babel/plugin-proposal-class-properties"]
    }
  },
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ]
  ]
}